{
  "name": "@gaiajs/grpc",
  "version": "1.0.1",
  "description": "",
  "egg": {
    "typescript": true,
    "declarations": true,
    "framework": "@gaiajs/gaiajs"
  },
  "eggPlugin": {
    "name": "gaia-grpc",
    "dependencies": [],
    "gaia-plugin": true
  },
  "scripts": {
    "dev": "egg-bin dev --baseDir=test/fixtures",
    "debug": "egg-bin debug --baseDir=test/fixtures",
    "test-local": "egg-bin test",
    "test": "npm run lint -- --fix && npm run test-local",
    "cov": "egg-bin cov",
    "tsc": "ets && tsc -p tsconfig.json",
    "ci": "npm run lint && npm run cov && npm run tsc",
    "autod": "autod",
    "lint": "npm run gencode && eslint . --ext .ts",
    "clean": "ets clean",
    "build": "npm run tsc",
    "script": "node node_modules/@gaiajs/gaiajs/bin/run.js",
    "pub": "rm -rf dist/ && npm run build && npm version patch && cp -Rf typings package.json index.* tshelper.js ./dist/ && npm publish dist/ --access public --registry=https://registry.npmjs.org/"
  },
  "dependencies": {
    "@gaiajs/gaiajs": "^1.0.0",
    "debug": "^4.1.1",
    "egg": "^2.6.1",
    "egg-scripts": "^2.6.0"
  },
  "devDependencies": {
    "@grpc/grpc-js": "^1.3.7",
    "@grpc/proto-loader": "^0.6.4",
    "@types/mocha": "^2.2.40",
    "@types/node": "^14.0.11",
    "@types/supertest": "^2.0.0",
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-ci": "^1.8.0",
    "egg-bin": "^4.11.0",
    "egg-mock": "^3.16.0",
    "tslib": "^1.9.0",
    "eslint": "^6.7.2",
    "eslint-config-egg": "^8.0.0",
    "tslint-microsoft-contrib": "^6.2.0",
    "typescript": "^4.3.5"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "ci": {
    "version": "8"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "eslintIgnore": [
    "coverage"
  ],
  "author": "yonggang.zhang",
  "license": "MIT"
}
